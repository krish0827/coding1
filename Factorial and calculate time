FACTORIAL:

def main():
  print(fact(5))

def fact(n):
  f=1
  for i in range(1,n+1):
    f*=i
  return f
if __name__=="__main__":
  main()

OR


def main():
  print(fact(4))

def fact(n):
  if n==0:
    return 1
    
  else:
    return fact(n-1)*n
  
if __name__=="__main__":
  main()

#To calculate time
def main():
  n=5
  start=datetime.now()
  factI(n)
  end=datetime.now()
  print(end-start)

 
  start=datetime.now()
  factII(n)
  end=datetime.now()
  print(end-start)
