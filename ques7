#print ABCDEF


def main():
  for i in range(6):
    print(chr(i+65),end=" ") #return the string
  
if __name__=="__main__":
  main()

#print pattern 
# A
  AB
  
def main():
  n=int(input())
  
  for i in range(1,n+1):
    for j in range(1,i+1):
      print(chr(j+64),end=" ")
    print()

if __name__=="__main__":
  main()
  
    
