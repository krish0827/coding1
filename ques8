#SUM oF NUMBER

def main():
  n=int(input())
  s=str(n)
  sum=0
  for ch in s:
    sum=sum+int(ch)
  print(sum)

if __name__=="__main__":
  main()

#sum using List comprehension


def main():
  n=int(input())
  s=sum([int(ch) for ch in str(n)])  #List comprehension
 
  print(s)
if __name__=="__main__":
  main()
